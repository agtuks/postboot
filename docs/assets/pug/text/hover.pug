extends ../example.pug

block example_title
  h1.font-weight-normal Hover

block example_result
  each text, variant in colors
    if variant != 'black'
      +text_hover(text, variant)

block example_code
  :encode-pug
    | <!-- Source -->
    each text, variant in colors
      if variant != 'black'
        div(class='text-hover-' + variant)= text
