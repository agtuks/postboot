extends ../example.pug

block example_title
  h1.font-weight-normal Custom

block example_result
  each text, variant in extraColors
    if variant != 'black' && variant != 'white'
      +text_variant(text, variant)

block example_code
  :encode-pug
    | <!-- Source -->
    each text, variant in extraColors
      if variant != 'black' && variant != 'white'
        div(class='text-' + variant)= text
