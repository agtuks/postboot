extends ../example.pug

block example_title
  h3.font-weight-normal Custom

block example_result
  .row
    each text, variant in extraColors
      if variant != 'black' && variant != 'white'
        .col
          +border_variant(text, variant)

block example_code
  :encode-pug
    | <!-- Source -->
    each text, variant in extraColors
      if variant != 'black' && variant != 'white'
        p(class='border-' + variant)= text
