extends ../example.pug

block example_title
  h3.fw-400 Custom

block example_result
  each text, variant in customColors
    if variant != 'white'
      +background_inverse(text, variant)

block example_code
  :encode-pug
    | <!-- Source -->
    each text, variant in customColors
      if variant != 'white'
        div(class='bg-' + variant)
