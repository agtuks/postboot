extends ../example.pug

block example_title
  h3.font-weight-normal Custom

block example_body
  .card-body
    each text, variant in extraColors
      if variant != 'black' && variant != 'white'
        +background_inverse(text, variant)

block example_code
  :encode-pug
    | <!-- Source -->
    each text, variant in extraColors
      if variant != 'black' && variant != 'white'
        p(class='bg-' + variant)= text
